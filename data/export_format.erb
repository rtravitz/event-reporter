<html>
<head>
  <meta charset='utf-8'>
  <title>Exported Data</title>
</head>
<body>
  <table>
    <tr>
      <% @headers.each do |header| %>
        <th> <%= "#{header}" %> </th>
      <% end %>
    </tr>
    <% @data.each do |person| %>
      <tr>
        <% @headers.each do |category| %>
          <th><%= "#{person.send(category)}" %></th>
        <% end %>
      </tr>
    <% end %>
  </table>
</body>
</html>
